		"controller.animation.walk": {
			"states": {
				"default": {
					"transitions": [
						{
							"walking": "(1.0)"
						}
					],
					"on_entry": [
                        "/function system/loop"
					]
				},
				"walking": {
					"transitions": [
						{
							"default": "(1.0)"
						}
					],
					"on_entry": [
